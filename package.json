{
  "name": "tdc-cdktf-k8s-app",
  "version": "1.0.0",
  "main": "src/main.js",
  "types": "src/main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "cdktf deploy tdc-dev-stack --auto-approve",
    "staging": "cdktf deploy tdc-staging-stack --auto-approve",
    "prod": "cdktf deploy tdc-production-stack --auto-approve",
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",

  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "11.0.10",
    "@cdktf/provider-kubernetes": "4.0.2",
    "cdktf": "^0.14.3",
    "constructs": "^10.2.45"
  },
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
